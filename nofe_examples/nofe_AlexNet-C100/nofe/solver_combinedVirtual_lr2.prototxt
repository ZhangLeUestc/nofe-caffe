net: "train_val_combinedVirtual.prototxt"

## 100 (test_iter) X 100 (batch_size) = 10,000 testing images
test_iter: 100

test_interval: 500  
base_lr: 0.00001     
lr_policy: "fixed"
gamma: 0.1
display: 20
max_iter: 70000  
momentum: 0.9
weight_decay: 0.004
snapshot: 500
snapshot_prefix: "snapshots/caffe_combined_train"
solver_mode: GPU
